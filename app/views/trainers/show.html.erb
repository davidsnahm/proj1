<h3>Trainer</h3>
<p>Name: <%= @trainer.name if @trainer.name %></p>
<p>Email: <%= @trainer.email if @trainer.email %></p>
<% @trainer.pokemon.each do |p| %>
	<p> Name: <%= p.name %> Level: <%= p.level %> Health: <%= p.health %> <%= button_to "Damage!", damage_path(id: p.id), :class => "button medium", :method => :patch %></p>
<% end %>
